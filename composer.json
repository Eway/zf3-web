{
    "name": "zendframework/zf3-web",
    "type": "project",
    "homepage": "https://framework.zend.com/",
    "support": {
        "issues": "https://github.com/zendframework/zf3-web/issues"
    },
    "license": "BSD-3-CLAUSE",
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1.3"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": "^7.1",
        "league/commonmark": "^0.15.3",
        "mnapoli/front-yaml": "^1.5.2",
        "ocramius/proxy-manager": "^2.0.4",
        "roave/security-advisories": "dev-master",
        "webuni/commonmark-table-extension": "^0.6.1",
        "zendframework/zend-config-aggregator": "^1.1",
        "zendframework/zend-dom": "^2.6",
        "zendframework/zend-expressive": "^3.0.0rc3",
        "zendframework/zend-expressive-helpers": "^5.0@alpha",
        "zendframework/zend-expressive-zendrouter": "^3.0.0rc3",
        "zendframework/zend-expressive-zendviewrenderer": "^2.0@alpha",
        "zendframework/zend-feed": "^2.8",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-stdlib": "^3.1"
    },
    "require-dev": {
        "filp/whoops": "^2.1.10",
        "guzzlehttp/guzzle": "^6.3",
        "phpunit/phpunit": "^7.0.1",
        "zendframework/zend-coding-standard": "~1.0.0",
        "zendframework/zend-component-installer": "^2.1",
        "zendframework/zend-expressive-tooling": "^1.0@alpha",
        "zfcampus/zf-development-mode": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Release\\": "src/Release/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
        "test": "phpunit"
    },
    "minimum-stability": "alpha"
}
